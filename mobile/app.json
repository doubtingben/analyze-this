{
  "expo": {
    "name": "Analyze This",
    "slug": "analyze-this",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "analyzethis",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.analyzethis.mobile",
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.analyzethis.mobile",
      "versionCode": 1,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "interestedparticipant.org",
              "pathPrefix": "/oauthredirect"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-share-intent",
        {
          "ios": {
            "activationRules": {
              "NSExtensionActivationSupportsWebURLWithMaxCount": 1,
              "NSExtensionActivationSupportsWebPageWithMaxCount": 1,
              "NSExtensionActivationSupportsText": true
            }
          },
          "android": {
            "intentFilters": [
              {
                "action": "android.intent.action.SEND",
                "data": {
                  "mimeType": "text/plain"
                }
              },
              {
                "action": "android.intent.action.SEND",
                "data": {
                  "mimeType": "text/*"
                }
              }
            ]
          },
          "scheme": "analyzethis"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7b70429d-ea0d-403d-aade-3d26f2f8bee8",
        "build": {}
      }
    },
    "owner": "doubtingben"
  }
}